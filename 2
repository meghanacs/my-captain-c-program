#include<stdio.h>
i#include<stdio.h>
int main()
{
	int marks;
	printf("ENTER THE THE MARKS\n");
	scanf("%d",&marks);
	if(marks>=85){
		printf("THE GRADE IS A");
	}
    if(marks>=70&&marks<=84){
		printf("THE GRADE IS B");
	}
    if(marks>=55&&marks<=69){
		printf("THE GRADE IS C");
	}
    if(marks>=40&&marks<=50){
		printf("THE GRADE IS D");
	}
	if(marks<=40){
		printf("THE GRADE IS F");
	}

}
