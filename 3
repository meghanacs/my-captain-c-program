#include<stdio.h>
int main()
{
	int choice; 
	printf("PICK THE RANDOM NUMBER BETWEEN 1 TO 5\n");
	scanf("%d",&choice);
	printf("YOUR CHOICE IS %d \n", choice);
	switch(choice){
	
	case 1 :
		printf("PIZZA Rs 239");
		break;
    case 2 :
		printf("BURGER Rs 129");
		break;
	case 3 :
		printf("PASTA Rs 179");
		break;
	case 4 :
		printf("FRENCH FRIES Rs 99");
		break;
	case 5 :
		printf("SANDWICH Rs 149");
		break;
	}
return 0;
}
